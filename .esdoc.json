{
  "source": "./lib",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-typescript-plugin",
      "option": {
        "enable": false
      }
    },
    {
      "name": "esdoc-standard-plugin"
    },
    {
      "name": "esdoc-inject-style-plugin",
      "option": {
        "enable": false,
        "styles": ["./bulma.css"]
      }
    }
  ]
}
